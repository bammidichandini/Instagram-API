{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mystoragenousdatafactory"
		},
		"ls_staging_storage_data_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_staging_storage_data'"
		},
		"ls_storage_csv_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_storage_csv'"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.hesa.ac.uk"
		},
		"ls_source_http_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.hesa.ac.uk/data-and-analysis/students/table-46"
		},
		"ls_staging_storage_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mystoragenousdata.dfs.core.windows.net/"
		},
		"ls_storage_csv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mystoragenousdata.dfs.core.windows.net/"
		},
		"suresh_csv_with_headers_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.hesa.ac.uk/data-and-analysis/students/table-46.csv?__cf_chl_tk=zma5MxOCjBvx80ATQHeMykUWaMdoiGNSzl91pyCcbYw-1743829301-1.0.1.1-Difo5gRTrdHO4wHGSSAUdcq553Sj_1JATuvJePdh3dk"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mystoragenousdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_staging_storage_data_accountKey": {
						"value": "[parameters('ls_staging_storage_data_accountKey')]"
					},
					"ls_storage_csv_accountKey": {
						"value": "[parameters('ls_storage_csv_accountKey')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"ls_source_http_data_properties_typeProperties_url": {
						"value": "[parameters('ls_source_http_data_properties_typeProperties_url')]"
					},
					"ls_staging_storage_data_properties_typeProperties_url": {
						"value": "[parameters('ls_staging_storage_data_properties_typeProperties_url')]"
					},
					"ls_storage_csv_properties_typeProperties_url": {
						"value": "[parameters('ls_storage_csv_properties_typeProperties_url')]"
					},
					"suresh_csv_with_headers_properties_typeProperties_url": {
						"value": "[parameters('suresh_csv_with_headers_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}